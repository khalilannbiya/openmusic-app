{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "camelcase": "off",
    "quotes": "off",
    "object-curly-newline": "off",
    "import/no-extraneous-dependencies": "off",
    "max-len": "off",
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "space-in-parens": "off"
  }
}
